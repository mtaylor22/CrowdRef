{
  "manifest_version": 2,

  "name": "CrowdRef",
  "description": "Let the crowd handle your citations.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://crowdref.atwebpages.com/*",
    "cookies",
    "tabs"
  ]
}
